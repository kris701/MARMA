{
  "Dependencies": [
    {
      "Name": "Fast Downward",
      "Description": "Generic Planner",
      "TargetLocation": "fast-downward",
      "SetupCalls": [
        {
          "Name": "Cloning repository",
          "Command": "git",
          "Arguments": "clone https://github.com/aibasel/downward ../fast-downward"
        },
        {
          "Name": "Building repository",
          "Command": "python",
          "Arguments": "build.py"
        }
      ]
    },
    {
      "Name": "Stackelberg Planner",
      "Description": "Stackelberg Planner",
      "TargetLocation": "stackelberg-planner",
      "SetupCalls": [
        {
          "Name": "Cloning repository",
          "Command": "git",
          "Arguments": "clone -n --depth=1 --filter=tree:0 https://gitlab.com/atorralba_planners/stackelberg-planner-sls ../stackelberg-planner"
        },
        {
          "Name": "Sparse checkout of repository",
          "Command": "git",
          "Arguments": "sparse-checkout set --no-cone src"
        },
        {
          "Name": "Final checkout of repository",
          "Command": "git",
          "Arguments": "checkout"
        },
        {
          "Name": "Move to build dir",
          "Command": "cd",
          "Arguments": "src"
        },
        {
          "Name": "Building repository",
          "Command": "bash",
          "Arguments": "build_all"
        }
      ]
    },
    {
      "Name": "Fast Downward Benchmarks",
      "Description": "A set of domains and problems that is tested on.",
      "TargetLocation": "downward-benchmarks-master",
      "SetupCalls": [
        {
          "Name": "Cloning repository",
          "Command": "git",
          "Arguments": "clone https://github.com/aibasel/downward-benchmarks ../downward-benchmarks-master"
        }
      ]
    }
  ]
}